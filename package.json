{
  "name": "riungnode",
  "version": "0.1.0",
  "description": "API Backend for ngariung app",
  "main": "index.js",
  "author": "Muhammad Nasrurrohman <anas@jurnalanas.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "NODE_ENV=testing jest --forceExit --detectOpenHandles  --silent",
    "test-routes": "yarn test -t router",
    "test-models": "yarn test -t model",
    "test-controllers": "yarn test -t controllers",
    "dev": "nodemon --exec yarn restart",
    "start": "nodemon dist/app.js",
    "lint": "eslint \"src/**/*.{js}\" --quiet",
    "restart": "rimraf dist && yarn build && yarn start"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express-validator": "^6.3.0",
    "mongoose": "^5.8.3",
    "mongoose-unique-validator": "^2.0.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "mock-req-res": "^1.1.4",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
