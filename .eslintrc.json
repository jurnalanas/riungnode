{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "prettier"
  ],
  "rules": {
    "react/prop-types": 0
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
